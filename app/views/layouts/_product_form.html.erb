<%= render "layouts/errors", :object => @product %>

<%= form_for @product do |f| %>
  <%= f.label :prod_name %>
  <%= f.text_field :prod_name %>
  <%= f.label :cost%>
  <%= f.text_field :cost %>
  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %>
  <%= f.submit %>
<% end %>
